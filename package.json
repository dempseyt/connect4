{
  "name": "connect4",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build:prod": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest --mode development",
    "devcontainer:test": "pnpm run build:dev --mode development && devcontainer up --workspace-folder . --remove-existing-container",
    "docker-compose-up:prod": "pnpm run build:prod && docker compose -f .devcontainer/docker-compose.yml up"
  },
  "dependencies": {
    "@types/ramda": "^0.30.0",
